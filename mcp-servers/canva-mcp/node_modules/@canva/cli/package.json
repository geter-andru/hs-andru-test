{
  "name": "@canva/cli",
  "version": "1.0.0",
  "description": "The official Canva CLI.",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Canva Pty Ltd.",
  "repository": {
    "directory": "git+https://github.com/canva-sdks/canva-cli.git",
    "type": "git"
  },
  "type": "module",
